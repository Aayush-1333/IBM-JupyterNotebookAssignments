{
    "name": "Python3.11 - Falcon9",
    "image": "mcr.microsoft.com/vscode/devcontainers/python:3.11",
    "customizations": {
        "settings": {
          "terminal.integrated.shell.linux": "/bin/bash"
        },
        "extensions": [
          "ms-python.python",
          "ms-python.vscode-pylance",
          "ms-toolsai.jupyter"
        ]
    },
    "postCreateCommand": "pip install -r requirements.txt",
    "remoteUser": "vscode"
}